{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng serve",
      "type": "msedge",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/"
    },
    {
      "name": "ng test",
      "type": "msedge",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    },
    {
      "type": "node-terminal",
      "name": "ng build prod",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "command": "ng build --prod --base-href \"https://visujomu.github.io/angular-test/\"",
      "cwd": "${workspaceFolder}"
    }
  ]
}